CHIP FullAdder {
    IN a, b, c;  // Input pins
    OUT sum, carry;  // Output pins


    PARTS:
    Xor(a=a, b=b, out=xor1_out);
    Xor(a=xor1_out, b=c, out=sum);


    And(a=a, b=b, out=and1_out);
    And(a=xor1_out, b=c, out=and2_out);
    Or(a=and1_out, b=and2_out, out=or1_out);
    Or(a=or1_out, b=c, out=carry);
}